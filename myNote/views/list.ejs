<%- include header %>

<div class="container">
 <div class="row">
  <table class="table table-striped">
  <caption><strong>我的笔记列表</strong></caption>
  <thead>
  <tr>
     <th>标题</th>
     <th>标签</th>
     <th >日期</th>
  </tr>
  </thead>
  <tbody>
  <% notes.forEach(function (note){%>
  <tr>
    <td>
      <a href="/detail/<%= note._id %>"><%=note.title %></a>
    </td>
    <td><%= note.tag%></td>
    <td><%= note.createTime%></td>
    <td><a href="/delete/<%= note._id %>"> 删除</a></td>
    <tr>
    <%});%>
 
  </tbody>
</table>
</div>
</div>


<%- include footer %>